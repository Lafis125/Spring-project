}

.css-category:after {
	content: "";
	display: block;
	position: absolute;
	left: 0;
	bottom: 0;
	width: 100%;
	height: 2px;
	background-color: #35c5f0;
	transition: transform 0.2s;
	transform: scale(0);
}

